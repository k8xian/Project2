<div class="start">
  <div class="rocketArea">
    <div class="rocket"></div>
    <a class="launch-mission" href="/current_missions" id="launch" value="Launch!">Launch Mission</a>
  </div>
  <div class="missionForm">
    <form class="newMissionData">
      <div>
        <label for="missionName">What is the name of your Mission?</label>
        <input type="text" id="missionName" name="missionName">
      </div>
      <div>
        <label for="noAstronauts">Who are your Astronauts?</label>
        <label for="astronautOneName" class="astronautName">Astronaut One</label>
        <input type="text" class="astronaut" id="astronautOneName" name="astronautFourName"><br>
        <label for="astronautTwoName" class="astronautName">Astronaut Two</label>
        <input type="text" class="astronaut" id="astronautTwoName" name="astronautFourName"><br>
        <label for="astronautThreeName" class="astronautName">Astronaut Three</label>
        <input type="text" class="astronaut" id="astronautThreeName" name="astronautFourName"><br>
        <label for="astronautFourName" class="astronautName">Astronaut Four</label>
        <input type="text" class="astronaut" id="astronautFourName" name="astronautFourName"><br>
      </div>
    </form>
    <a class="submit-mission" id="createMission">Create Mission!</a>
    <div class="notice" id="notice">Mission Created!<br>Click launch to begin</div>
  </div>
</div>

